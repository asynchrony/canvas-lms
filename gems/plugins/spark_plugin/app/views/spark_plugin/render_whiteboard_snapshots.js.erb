require([
  'jquery' /* $ */
], function($) {
  $('#display_whiteboard_snapshots').html("<%= escape_javascript(render partial: 'whiteboard_snapshots', locals: {course_id: @course_id, module_id: @module_id, whiteboards: @whiteboards})%>");

  $('.whiteboard').click(function() {
    $('.selected').removeClass('selected');
    $(this).addClass('selected');

    $('.add_item_button').removeClass('disabled').attr('aria-disabled', false);
  });
});
